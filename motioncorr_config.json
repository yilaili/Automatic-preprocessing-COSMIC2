{
  "general": {
    "input": "--i movies.star",
    "output": "--o MotionCorr/ ",
    "runout": "> run_motioncorr.out ",
    "runerr": "2> run_motioncorr.err "
  },
  "motioncorr2": {
    "module": "module load relion/3.0-cluster/openmpi/3.1.2",
    "extra": "",
    "command": "mpirun  -np 10 `which relion_run_motioncorr_mpi` ",
    "parameters": "--first_frame_sum 1 --last_frame_sum -1 --use_own  --j 3 --bin_factor 1 --bfactor $$bfactor --angpix $$angpix --voltage $$voltage --dose_per_frame 1 --preexposure 0 --patch_x $$patch_x --patch_y $$patch_y --gainref $$gainref --gain_rot 0 --gain_flip 0  ",
    "tail": "< /dev/null"
  }
}
