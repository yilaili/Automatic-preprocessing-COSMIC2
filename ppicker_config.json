{
  "general": {
    "input": "-i test/ ",
    "output": "--o boxfiles/ ",
    "runout": "> run_cryolo.out ",
    "runerr": "2> run_cryolo.err "
  },
  "cryolo": {
    "module": "module load python-anaconda3/latest",
    "extra": "source activate cryolo",
    "command": "cryolo_predict.py ",
    "parameters": "-c $$config -w $$model -t $$thresh -d $$distance ",
    "tail": "< /dev/null"
  }
}
